procedure TFrm_Imp_4129.QRDBText10Print(sender: TObject;
  var Value: String);
begin
  inherited;
  if FQtdeCons1 = 0 then
    FQtde1 := 0
  else
  FQtde1 := fdt_principal5.spc_cons_med_custoAgregadoPorMedico.FieldByName('qtde1').AsInteger / FQtdeCons1;
  FtlQtde1 := FtlQtde1 + FQtde1;
  value := FormatFloat('###,###,##0.00',FQtde1);
end;

